{
    "config_version": 5,
    "git_registry": {
        "open-source": "https://github.com/ow-gryphon/template_registry.git",
        "ow-private": ""
    },
    "local_registry": {
        "local_default": "/home/vittorfp/blue-orange/labskit_registry"
    },
    "local_templates": [
        "/home/vittorfp/blue-orange/gryphon-analytics-basic"
    ],
    "template_indexes": [
        {
            "name": "grypi",
            "url": "https://ow-gryphon.github.io/grypi/",
